<Window x:Class="Hitbox.Window_error"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Hitbox"
        mc:Ignorable="d"
        Title="Window_error" Height="200" Width="300">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition Height="0.30*"/>
        </Grid.RowDefinitions>
        <Label Content="{Binding StringError}" Margin="35 25 35 25"/>
        <Button Content="OK" Grid.Row="1" Grid.Column="0" Command="{Binding OKCommandError}" Margin="40 0 40 12"/>
    </Grid>
</Window>
